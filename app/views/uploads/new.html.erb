<%= form_for @upload, html: { multipart: true } do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :category %>
  <%= f.select :category, ['Chambers', 'Court Family', 'Forms', 'Happenings', 'Home', 'HR', 'IT', 'Jury', 'Operations']%>
  <%= f.label :subcategory %>
  <%= f.select :subcategory, ['A','B','C']%>
  <%= f.label :attachment %>
  <%= f.file_field :attachment, class: "usa-button-gray" %>
  <%= f.submit "Upload" %>
<% end %>
